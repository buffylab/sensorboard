{
  "name": "sensorboard-host",
  "productName": "Sensorboard",
  "version": "0.1.0",
  "description": "Sensorboard host application",
  "main": "out/prod/main.js",
  "scripts": {
    "lint": "eslint app test *.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js ./test/e2e.js",
    "preinstall": "echo 'Skip default node-gyp rebuild'",
    "build:addon": "HOME=~/.electron-gyp node-gyp rebuild --target=$npm_package_devDependencies_electron_prebuilt --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "build:main": "webpack --config webpack/main.prod.js --progress --profile --colors",
    "build:renderer": "webpack --config webpack/renderer.prod.js --progress --profile --colors",
    "build:service": "webpack --config webpack/service.prod.js --progress --profile --colors",
    "build": "npm run build:addon && npm run build:main && npm run build:renderer && npm run build:service",
    "prepublish": "electron-rebuild && npm run build",
    "start": "gulp run",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buffylab/sensorboard-host.git"
  },
  "author": {
    "name": "Giff Song",
    "email": "giff.song@gmail.com",
    "url": "https://github.com/pawsong"
  },
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.10.0",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-syntax-decorators": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-decorators": "^6.6.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "0.37.2",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.5.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.1",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.15.0",
    "jsdom": "^8.2.0",
    "json-loader": "^0.5.4",
    "material-design-icons": "^2.2.3",
    "material-ui": "^0.15.0-alpha.2",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "nan": "^2.2.1",
    "node-libs-browser": "^1.0.0",
    "node-notifier": "^4.5.0",
    "react-addons-test-utils": "^0.14.7",
    "react-addons-update": "^0.14.8",
    "react-tap-event-plugin": "^0.2.2",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-logger": "^2.6.1",
    "roboto-fontface": "^0.4.5",
    "scriptjs": "^2.5.8",
    "selenium-webdriver": "^2.53.1",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "superb": "^1.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "bindings": "^1.2.1",
    "bunyan": "^1.8.0",
    "css-modules-require-hook": "^4.0.0",
    "electron-debug": "^0.5.2",
    "express": "^4.13.4",
    "font-awesome": "^4.5.0",
    "postcss": "^5.0.19",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "socket.io": "^1.4.5",
    "source-map-support": "^0.4.0"
  }
}
